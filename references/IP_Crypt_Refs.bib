@ARTICLE{AaWo2010,
author="Aardal, Karen and Wolsey, Laurence A.",
title="Lattice based extended formulations for integer linear equality systems",
journal="Mathematical Programming",
year="2010",
volume="121",
number="2",
pages="337",
abstract="We study different extended formulations for the set                                                                           {\$}{\$}{\{}X = {\backslash}{\{}{\{}{\backslash}boldsymbol x{\}}{\backslash}in {\backslash}mathbb{\{}Z{\}}^n {\backslash}mid {\{}{\backslash}boldsymbol A{\}}{\{}{\backslash}boldsymbol x{\}} = {\{}{\backslash}boldsymbol A{\}}{\{}{\backslash}boldsymbol x{\}}^0{\backslash}{\}}{\}}{\$}{\$}                 with                                                                           {\$}{\$}{\{}{\{}{\backslash}boldsymbol A{\}} {\backslash}in {\backslash}mathbb{\{}Z{\}}^{\{}m {\backslash}times n{\}}{\}}{\$}{\$}                 in order to tackle the feasibility problem for the set                                                                           {\$}{\$}{\{}X {\backslash}cap {\backslash}mathbb{\{}Z{\}}^n{\_}+.{\}}{\$}{\$}                 Pursuing the work of Aardal, Lenstra et al. using the reformulation                                                                           {\$}{\$}{\{}X={\backslash}{\{}{\{}{\backslash}boldsymbol x{\}} {\backslash}in {\backslash}mathbb{\{}Z{\}}^n {\backslash}mid {\{}{\backslash}boldsymbol x{\}}-{\{}{\backslash}boldsymbol x{\}}^0={\{}{\backslash}boldsymbol Q{\}}{\{}{\backslash}boldsymbol {\backslash}lambda{\}},{\backslash},{\{}{\backslash}boldsymbol {\backslash}lambda{\}} {\backslash}in {\backslash}mathbb{\{}Z{\}}^{\{}n-m{\}}{\backslash}{\}}{\}}{\$}{\$}                 , our aim is to derive reformulations of the form                                                                           {\$}{\$}{\{}{\backslash}{\{}{\{}{\backslash}boldsymbol x{\}} {\backslash}in {\backslash}mathbb{\{}Z{\}}^n {\backslash}mid {\{}{\backslash}boldsymbol P{\}}({\{}{\backslash}boldsymbol x{\}}-{\{}{\backslash}boldsymbol x{\}}^0)={\{}{\backslash}boldsymbol T{\}} {\{}{\backslash}boldsymbol {\backslash}mu{\}}, {\{}{\backslash}boldsymbol {\backslash}mu{\}} {\backslash}in {\backslash}mathbb{\{}Z{\}}^s{\backslash}{\}}{\}}{\$}{\$}                 with 0  ≤  s ≤ n − m where preferably all the coefficients of P are small compared to the coefficients of A and T. In such cases the new variables $\mu$ appear to be good branching directions, and in certain circumstances permit one to deduce rapidly that the instance is infeasible. We give a polynomial time algorithm for identifying such P, T if possible, and for the case that A has one row a we analyze the reformulation when s = 1, that is, one $\mu$-variable is introduced. In particular, we determine the integer width of the extended formulations in the direction of the $\mu$-variable, and derive a lower bound on the Frobenius number of a. We conclude with some preliminary tests to see if the reformulations are effective when the number s of additional constraints and variables is limited.",
issn="1436-4646",
doi="10.1007/s10107-008-0236-7",
url="http://dx.doi.org/10.1007/s10107-008-0236-7"
}

